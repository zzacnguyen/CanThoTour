    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.Favorite.ActivityFavorite"></activity>
        <activity android:name=".View.Notify.ActivityNotify"></activity>
        <activity android:name=".View.Personal.ActivityPersonal"></activity>
        <activity android:name=".View.Main.ActivityServiceInfo"></activity>
        <activity android:name=".View.Main.Content.ActivityService"></activity>
        <activity android:name=".View.Personal.ActivityAddPlace"></activity>
        <activity android:name=".View.Search.ActivitySearch"></activity>
        <activity android:name=".View.Personal.ActivityRegCoop"></activity>
        <activity android:name=".View.Personal.ActivityOption"></activity>
        <activity android:name=".View.Search.ActivityNearLocation"></activity>
        <activity android:name=".View.Personal.ActivityAddService"></activity>
        <activity android:name=".View.Personal.ActivityLogin"></activity>
    </application>

</manifest>